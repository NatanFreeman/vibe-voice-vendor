# VibeVoice ASR Server Configuration
# Copy this to .env and fill in values

# vLLM server URL (running in Docker)
VVV_VLLM_BASE_URL=http://localhost:37845

# FastAPI server bind address (vvv_proxy proxies to this)
VVV_SERVER_HOST=127.0.0.1
VVV_SERVER_PORT=54912

# Maximum upload size in bytes (default: 500MB)
VVV_MAX_AUDIO_BYTES=524288000

# Maximum number of queued jobs
VVV_MAX_QUEUE_SIZE=50

# Comma-separated bcrypt hashes of bearer tokens
# Generate with: uv run python -m scripts.generate_token
VVV_TOKEN_HASHES_ENV=$2b$12$EXAMPLE_HASH_1,$2b$12$EXAMPLE_HASH_2

# vLLM model settings
VVV_VLLM_MODEL_NAME=vibevoice
VVV_VLLM_MAX_TOKENS=65536
VVV_VLLM_TEMPERATURE=0.0
VVV_VLLM_TOP_P=1.0

